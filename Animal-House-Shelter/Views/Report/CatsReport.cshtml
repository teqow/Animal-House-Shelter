@model List<Cat>
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <title>Cats Report</title>
    <meta charset="utf-8">
</head>
<body>
    <div class="container">
        <h2>Cats List</h2>
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th>CatID</th>
                    <th>Name</th>
                    <th>Breed</th>
                    <th>Age</th>
                    <th>Weight</th>
                    <th>Gender</th>
                </tr>
            </thead>
            <tbody>

                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.CatID</td>
                        <td>@item.Name</td>
                        <td>@item.Breed</td>
                        <td>@item.Age</td>
                        <td>@item.Weight</td>
                        <td>@item.Gender</td>
                    </tr>
                }

            </tbody>
        </table>
    </div>
</body>
</html> 